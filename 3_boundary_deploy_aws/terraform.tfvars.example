# AWS Region
region = "eu-west-2"

# VPC and Network (auto-discovered from 1_create_clusters if not specified)
# vpc_id     = ""
# subnet_ids = []

# SSH Key (use the same key from 1_create_clusters)
key_pair_name = "vault-key-xxxx"  # Replace xxxx with your random suffix

# DNS Configuration
dns_zone_name = "example.com"  # Replace with your domain
owner_email   = "admin@example.com"

# Boundary Configuration
boundary_version = "0.18.1+ent"
boundary_license = ""
cluster_name     = "boundary"

# Database Configuration
db_username = "boundary"
db_password = "ChangeThisSecurePassword123!"  # CHANGE THIS!

# Vault Configuration (from 1_create_clusters outputs)
vault_addr      = "https://vault-server-public-ip:8200"  # Replace with actual Vault server IP or FQDN
vault_token     = "hvs.XXXXX"                            # Use root token or token with admin permissions
vault_namespace = ""                                     # Leave empty if not using Vault Enterprise namespaces

# ACME/Let's Encrypt
acme_prod = false  # Set to true for production certificates

# Instance Types
controller_instance_type = "t3.medium"
worker_instance_type     = "t3.medium"

# Additional Tags
tags = {
  Terraform   = "true"
  Environment = "demo"
  Project     = "boundary"
  Owner       = "team-platform"
}
